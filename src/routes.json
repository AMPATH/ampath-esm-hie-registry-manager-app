{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {},
  "pages": [
    {
      "component": "facilityRegistryRoot",
      "route": "hie/facilities",
      "online": true,
      "offline": false
    },
    {
      "component": "healthWorkerRegistryRoot",
      "route": "hie/health-workers",
      "online": true,
      "offline": false
    },
     {
      "component": "shrDetails",
      "route": "hie/shared-health-records",
      "online": true,
      "offline": false
    }
  ],
  "extensions": [
    {
      "component": "hieNavLink",
      "name": "hie-registry-nav",
      "slot": "app-menu-slot",
      "online": true,
      "offline": false,
      "order": 70
    },
     {
      "name": "shr-summary-dashboard",
      "slot": "patient-chart-dashboard-slot",
      "component": "shrDashboardLink",
      "meta": {
        "slot": "patient-chart-shr-summary-dashboard-slot",
        "path": "SHR",
        "layoutMode": "anchored"
      },
      "order": 5,
      "online": true,
      "offline": true
    },
    {
      "name": "shr-details-overview",
      "slot": "patient-chart-shr-summary-dashboard-slot",
      "component": "shrDetails",
      "order": 1,
      "meta": {
        "title": "SHR",
        "view": "shr"
      },
      "online": true,
      "offline": true
    },
    {
      "component": "healthWorkerBanner",
      "name": "hie-health-worker-banner",
      "slot": "patient-header-slot",
      "online": true,
      "offline": false
    }
  ],
  "modals": []
}
